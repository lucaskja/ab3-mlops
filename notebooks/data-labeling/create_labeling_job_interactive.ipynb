    "            manifest_content = response['Body'].read().decode('utf-8')\n",
    "            \n",
    "            # Parse the manifest (each line is a JSON object)\n",
    "            annotations = []\n",
    "            for line in manifest_content.strip().split('\\n'):\n",
    "                if line:\n",
    "                    annotations.append(json.loads(line))\n",
    "            \n",
    "            # Extract label distribution\n",
    "            label_counts = {}\n",
    "            annotations_per_image = []\n",
    "            \n",
    "            for annotation in annotations:\n",
    "                # Find the label field (usually ends with \"-labels\")\n",
    "                label_field = None\n",
    "                for key in annotation.keys():\n",
    "                    if key.endswith('-labels'):\n",
    "                        label_field = key\n",
    "                        break\n",
    "                \n",
    "                if label_field and isinstance(annotation[label_field], dict) and 'annotations' in annotation[label_field]:\n",
    "                    # Count annotations per image\n",
    "                    image_annotations = annotation[label_field]['annotations']\n",
    "                    annotations_per_image.append(len(image_annotations))\n",
    "                    \n",
    "                    # Count labels\n",
    "                    for box in image_annotations:\n",
    "                        if 'label' in box:\n",
    "                            label = box['label']\n",
    "                            if label not in label_counts:\n",
    "                                label_counts[label] = 0\n",
    "                            label_counts[label] += 1\n",
    "            \n",
    "            # Display label distribution\n",
    "            print(f\"\\nAnalyzed {len(annotations)} images with {sum(label_counts.values())} annotations\")\n",
    "            print(f\"Average annotations per image: {sum(annotations_per_image) / len(annotations_per_image):.2f}\")\n",
    "            \n",
    "            # Create a bar chart for label distribution\n",
    "            plt.figure(figsize=(10, 6))\n",
    "            plt.bar(label_counts.keys(), label_counts.values())\n",
    "            plt.title('Label Distribution')\n",
    "            plt.xlabel('Label')\n",
    "            plt.ylabel('Count')\n",
    "            plt.xticks(rotation=45)\n",
    "            plt.tight_layout()\n",
    "            plt.show()\n",
    "            \n",
    "            # Create a histogram for annotations per image\n",
    "            plt.figure(figsize=(10, 6))\n",
    "            plt.hist(annotations_per_image, bins=range(0, max(annotations_per_image) + 2))\n",
    "            plt.title('Annotations per Image')\n",
    "            plt.xlabel('Number of Annotations')\n",
    "            plt.ylabel('Number of Images')\n",
    "            plt.xticks(range(0, max(annotations_per_image) + 1))\n",
    "            plt.tight_layout()\n",
    "            plt.show()\n",
    "            \n",
    "            # Update status\n",
    "            update_status(\"Label distribution analysis complete\", 8)\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"Error analyzing label distribution: {str(e)}\")\n",
    "            update_status(f\"Error analyzing label distribution: {str(e)}\", 8)\n",
    "\n",
    "# Connect button click events\n",
    "generate_viz_button.on_click(generate_visualizations)\n",
